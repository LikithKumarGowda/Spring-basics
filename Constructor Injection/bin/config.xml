<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.Student" name="s">
		<constructor-arg name="studentAge" value="20"></constructor-arg>
		<constructor-arg name="studentName" value="Raj">
		</constructor-arg>
	</bean>
	<!-- to comment the above block use ctrl+Shift+ / . and \ for de block -->
	
	<bean class="com.Employee" name="e">
		<constructor-arg name="companyName" value="LNT"></constructor-arg>
		<constructor-arg name="names" >
			<list>
				<value>Raj</value>
				<value>Ravi</value>
				<value>Ramesh</value>
			</list>
		</constructor-arg>
		<constructor-arg name="ids" >
			<set>
				<value>12</value>
				<value>23</value>
				<value>34</value>
			</set>
		</constructor-arg>
	</bean>


</beans>